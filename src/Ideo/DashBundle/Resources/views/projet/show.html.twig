{% extends 'base.html.twig' %}

{% block content %}
    <h1>Projet</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ projet.id }}</td>
            </tr>
            <tr>
                <th>Idprojet</th>
                <td>{{ projet.idProjet }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ projet.code }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ projet.nom }}</td>
            </tr>
            <tr>
                <th>Datelancement</th>
                <td>{% if projet.dateLancement %}{{ projet.dateLancement|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Datefin</th>
                <td>{% if projet.dateFin %}{{ projet.dateFin|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Effectifprevu</th>
                <td>{{ projet.effectifPrevu }}</td>
            </tr>
            <tr>
                <th>Clientid</th>
                <td>{{ projet.clientId }}</td>
            </tr>
            <tr>
                <th>Idstat</th>
                <td>{{ projet.idStat }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('projet_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('projet_edit', { 'id': projet.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
